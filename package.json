{
  "name": "cig",
  "version": "0.1.0",
  "description": "Call typescript methods with natural language and get the response back in specified structure",
  "main": "src/index.js",
  "scripts": {
    "format": "deno fmt",
    "test": "vitest",
    "test:loud": "vitest --reporter=basic --disable-console-intercept",
    "publish": "npx jsr publish --dry-run",
    "example:weather": "npx ts-node examples/testWeather.ts",
    "example:classification": "npx ts-node examples/testClassification.ts",
    "example:simple": "npx ts-node examples/testSimple.ts",
    "example:extraction": "npx ts-node examples/testExtraction.ts",
    "example:generation": "npx ts-node examples/testGeneration.ts",
    "example:chaining": "npx ts-node examples/testChaining.ts",
    "example:handler": "npx ts-node examples/testHandler.ts",
    "example:uses": "npx ts-node examples/testUses.ts",
    "example:super-simple": "npx ts-node examples/testSuperSimple.ts"
  },
  "author": "Jordan Howlett <jordan@howlett.io>",
  "license": "MIT",
  "dependencies": {
    "eta": "^3.5.0",
    "gpt-tokenizer": "^2.2.1",
    "openai": "^4.56.0",
    "tslog": "^4.9.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "vitest": "^2.0.5"
  }
}
