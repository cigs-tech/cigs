{
  "compilerOptions": {
    "target": "ESNext",
    "moduleResolution": "NodeNext",  
    "module": "NodeNext",
    "allowImportingTsExtensions": true,
    "noEmit": true,
		"noImplicitAny": false,
    "strict": true,
		"skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
    "allowJs": true,
		"declaration": true,
		"declarationMap": true,
		"sourceMap": true,
  }
}
